{
  "images": {
    "sizes": [640, 750, 828, 1080, 1200, 1920, 2048, 3840],
    "minimumCacheTTL": 60
  },
  "headers": [
    {
      "source": "/content/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/video/(.*)",
      "headers": [
        {
          "key": "cache-control", 
          "value": "public, max-age=86400"
        }
      ]
    }
  ]
}